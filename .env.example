#--------------------------------------------------------------------------
# Database Configuration - Profile Selection
#--------------------------------------------------------------------------
# DB_PROFILE:
#   1 = Home (local MySQL on personal laptop)
#   2 = Office (SQL Server on office network)
#
DB_PROFILE=1

#-------------- MySQL Configuration (for home) --------------
DB_MYSQL_HOST=127.0.0.1
DB_MYSQL_PORT=3306
DB_MYSQL_DATABASE=spfi_ms
DB_MYSQL_USERNAME=root
DB_MYSQL_PASSWORD=

#-------------- SQL Server Configuration (for office) --------------
DB_SQLSRV_HOST=192.168.11.250
DB_SQLSRV_PORT=1433
DB_SQLSRV_DATABASE=spfi_ms
DB_SQLSRV_USERNAME=sa
DB_SQLSRV_PASSWORD=YourPasswordHere
DB_SQLSRV_ENCRYPT=no
DB_SQLSRV_TRUST_SERVER_CERTIFICATE=true

# Legacy connection fallback (for backward compatibility)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=spfi_ms
DB_USERNAME=root
DB_PASSWORD=

